package com.web.rest.dto;




import java.util.List;

import com.fasterxml.jackson.databind.annotation.JsonSerialize;

/**
 * @author Ankur
 */

@JsonSerialize(include = JsonSerialize.Inclusion.NON_NULL)
public class GameInfoDto {

	private String balanceCoinsText;
	private int balanceCoins;
	private List<String> infoText;
	private VideoDto videoDto;
	private EarnMoreCoinsDto earnMoreCoinsDto;
	private String tickerText;
	private String errorText;
	private String buttonName;
	private int timer;
	
	
	public String getButtonName() {
		return buttonName;
	}
	public void setButtonName(String buttonName) {
		this.buttonName = buttonName;
	}
	public int getTimer() {
		return timer;
	}
	public void setTimer(int timer) {
		this.timer = timer;
	}
	public String getBalanceCoinsText() {
		return balanceCoinsText;
	}
	public void setBalanceCoinsText(String balanceCoinsText) {
		this.balanceCoinsText = balanceCoinsText;
	}
	public int getBalanceCoins() {
		return balanceCoins;
	}
	public void setBalanceCoins(int balanceCoins) {
		this.balanceCoins = balanceCoins;
	}
	public List<String> getInfoText() {
		return infoText;
	}
	public void setInfoText(List<String> infoText) {
		this.infoText = infoText;
	}
	public VideoDto getVideoDto() {
		return videoDto;
	}
	public void setVideoDto(VideoDto videoDto) {
		this.videoDto = videoDto;
	}
	public EarnMoreCoinsDto getEarnMoreCoinsDto() {
		return earnMoreCoinsDto;
	}
	public void setEarnMoreCoinsDto(EarnMoreCoinsDto earnMoreCoinsDto) {
		this.earnMoreCoinsDto = earnMoreCoinsDto;
	}
	public String getTickerText() {
		return tickerText;
	}
	public void setTickerText(String tickerText) {
		this.tickerText = tickerText;
	}
	public String getErrorText() {
		return errorText;
	}
	public void setErrorText(String errorText) {
		this.errorText = errorText;
	}	
}
