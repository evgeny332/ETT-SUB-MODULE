package com.web.rest.dto;

import java.util.List;

import com.fasterxml.jackson.databind.annotation.JsonSerialize;

/**
 * @author Ankur
 */
// TODO:: status must have with tags or message
@JsonSerialize(include = JsonSerialize.Inclusion.NON_NULL)
public class CategorizedOffersDto {
	private String root;
	
	private String colorCode;
	
	private String inviteAlertText;
	
	private List<OffersDto> offersDtos;
	
	private int offerRefreshTimer;
	
	
	
	
	
	public int getOfferRefreshTimer() {
		return offerRefreshTimer;
	}

	public void setOfferRefreshTimer(int offerRefreshTimer) {
		this.offerRefreshTimer = offerRefreshTimer;
	}

	public String getColorCode() {
		return colorCode;
	}

	public void setColorCode(String colorCode) {
		this.colorCode = colorCode;
	}

	public String getInviteAlertText() {
		return inviteAlertText;
	}

	public void setInviteAlertText(String inviteAlertText) {
		this.inviteAlertText = inviteAlertText;
	}

	public String getRoot() {
		return root;
	}

	public void setRoot(String root) {
		this.root = root;
	}

	public List<OffersDto> getOffersDtos() {
		return offersDtos;
	}

	public void setOffersDtos(List<OffersDto> offersDtos) {
		this.offersDtos = offersDtos;
	}
	
	
}
