function setLength(str)
{
	if(str.length < 2)
		return "0"+str;
	else
		return str;
}
function trim(str)
{
	return 	str.replace("/ /g","");
}
function TrimMessage(str)
{
	while (str.substring(0,1) == ' ')
	{
		str = str.substring(1, str.length);
	}
	while(str.substring(str.length-1,str.length) == ' ')
	{
		str = str.substring(0,str.length-1);
	}
	return str;
}
